{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "generate": "turbo run generate",
    "init": "turbo run build --filter '@repo/*'",
    "publish-packages": "pnpm generate && turbo run build lint test && changeset version && changeset publish",
    "reset": "npx npkill"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@repo/eslint-config": "workspace:*",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "dotenv-cli": "7.3.0",
    "eslint": "8.0.0",
    "jest": "^29.7.0",
    "npkill": "^0.11.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "tsup": "^8.2.4",
    "tsx": "^4.18.0",
    "turbo": "^2.0.14",
    "typescript": "5.4.5",
    "vitest": "^2.0.5"
  },
  "resolutions": {
    "jose": "5.9.6"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@9.8.0"
}